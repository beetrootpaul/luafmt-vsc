{
	"name": "luafmt",
	"displayName": "PaBa TEST luafmt",
	"description": "PaBa TEST Lua formatter / pretty printer",
	"publisher": "appguru",
	"version": "0.0.16",
	"engines": {
		"vscode": "^1.48.0"
	},
	"keywords": [
		"Lua",
		"Formatter",
		"Pretty-Printer"
	],
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"title": "luafmt",
			"properties": {
				"luafmt.extra_newlines": {
					"type": "boolean",
					"default": true,
					"description": "Newlines around function declarations"
				},
				"luafmt.inline.block.max_exp_length": {
					"type": "number",
					"default": 60,
					"description": "Max expression length for inline blocks with single expressions"
				},
				"luafmt.inline.block.max_field_count": {
					"type": "number",
					"default": 3,
					"description": "Max field count for inlining tables"
				},
				"luafmt.inline.block.max_field_length": {
					"type": "number",
					"default": 60,
					"description": "Max length of each serialized field in characters"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint"
	},
	"dependencies": {
		"luafmt-pico8": "file:../luafmt-pico8"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.0",
		"@types/node": "^14.14.19",
		"@types/vscode": "^1.48.0",
		"eslint": "^7.17.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.1"
	},
	"repository": {
		"url": "https://github.com/appgurueu/luafmt-vsc"
	},
	"license": "MIT",
